 <listener>
  <listener-class>com.gy.timer.TerOnlineMonitorListner</listener-class>
 </listener>
 <servlet>
  <description>This is the description of my J2EE component</description>
  <display-name>This is the display name of my J2EE component</display-name>
  <servlet-name>QueryCars</servlet-name>
  <servlet-class>com.gy.CarMonitor.QueryCars</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>OracleMonitor</servlet-name>
  <servlet-class>com.gy.CarMonitor.OracleMonitor</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>RealMonitor</servlet-name>
  <servlet-class>com.gy.CarMonitor.RealMonitor</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>OffLineMonitor</servlet-name>
  <servlet-class>com.gy.CarMonitor.OffLineMonitor</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>ViewMap</servlet-name>
  <servlet-class>com.gy.CarMonitor.ViewMap</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>AbnormalMonitor</servlet-name>
  <servlet-class>com.gy.CarMonitor.AbnormalMonitor</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>QueryCars</servlet-name>
  <url-pattern>/QueryCars</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>RealMonitor</servlet-name>
  <url-pattern>/RealMonitor</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>OracleMonitor</servlet-name>
  <url-pattern>/OracleMonitor</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>OffLineMonitor</servlet-name>
  <url-pattern>/OffLineMonitor</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ViewMap</servlet-name>
  <url-pattern>/ViewMap</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>AbnormalMonitor</servlet-name>
  <url-pattern>/AbnormalMonitor</url-pattern>
 </servlet-mapping>
 <welcome-file-list>
  <welcome-file>index.jsp</welcome-file>
 </welcome-file-list>
 <login-config>
  <auth-method>BASIC</auth-method>
 </login-config>
</web-app>
